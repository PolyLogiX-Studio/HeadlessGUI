{
  "name": "headlesscore",
  "productName": "HeadlessGUI",
  "version": "1.0.1-alpha.1",
  "description": "Headless Server for Neos",
  "main": "start.js",
  "scripts": {
    "test": "exit 0",
    "start": "electron .",
    "package-linux-x64": "electron-packager . \"HeadlessGUI\" --overwrite --asar --platform=linux --arch=x64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"HeadlessGUI\"",
    "package-win32-x64": "electron-packager . \"HeadlessGUI\" --overwrite --asar --platform=win32 --arch=x64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"HeadlessGUI\""
  },
  "author": "Ethan Snow",
  "license": "GNU GPLv3",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "compare-versions": "^3.5.1",
    "electron": "^7.1.5",
    "electron-context-menu": "^0.15.1",
    "electron-css-reload": "^1.0.8",
    "electron-debug": "^3.0.1",
    "electron-dl": "^2.0.0",
    "electron-store": "^5.1.0",
    "electron-unhandled": "^3.0.1",
    "electron-util": "^0.13.1",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.3",
    "localized-strings": "^0.2.4",
    "markdown-it": "^10.0.0",
    "node-fetch": "^2.6.0",
    "popper.js": "^1.16.0",
    "root-require": "^0.3.1",
    "url-exists": "^1.0.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "electron-packager": "^14.1.1"
  }
}
